    <selectionEntry name="Grenade Launcher - Frag Charge" type="upgrade" id="1bb9-b239-4221-b16e">
      <infoLinks>
        <infoLink name="Blast" hidden="false" type="rule" targetId="1d9a-73ef-5f4f-8bd8" id="f123-0fd0-4d2a-92ba">
          <modifiers>
